import 'package:mine/display/multi_mine_board_display.dart';
import 'package:mine/display/game_display.dart';
import 'package:mine/game/multi_mine_game.dart';

class MultiMineGameDisplay extends GameDisplay {
  final MultiMineGame game;
  MultiMineBoardDisplay get boardDisplay => MultiMineBoardDisplay(game);

  MultiMineGameDisplay(this.game);
}